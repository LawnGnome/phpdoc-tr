<refentry xml:id='function.domdocumenttype-system-id' xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DomDocumentType::system_id</refname>
  <refpurpose>Belge türünün sistem betimleyicisini döndürür</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>DomDocumentType::system_id</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Belge türünün sistem betimleyicisini döndürür.
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Belge türünün sistem betimleyicisini bir dizge olarak döndürür.
  </para>
 </refsect1>
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>- Sistem betimleyicisini döndürmek</title>
    <programlisting role="php">
<![CDATA[
<?php
include("example.inc");

if (!$dom = domxml_open_mem($xmlstr)) {
  echo "Belge çözümlenirken hata oluştu\n";
  exit;
}

$doctype = $dom->doctype();
echo $doctype->system_id();
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
/share/sgml/Norman_Walsh/db3xml10/db3xml10.dtd
]]>
    </screen>
   </example>
  </para>
 </refsect1>
 <refsect1 role="migration">
  <title>PHP 5&apos;e yükseltme</title>
  <para>
   <classname>DOMDocumentType</classname> nesnesinin
   <property>systemId</property> özelliğini kullanın.
  </para>
 </refsect1>
</refentry>

