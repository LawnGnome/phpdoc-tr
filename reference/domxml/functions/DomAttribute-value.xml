<refentry xml:id='function.domattribute-value' xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DomAttribute::value</refname>
  <refpurpose>Özniteliğin değerini döndürür</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
   <methodsynopsis>
    <type>string</type><methodname>DomAttribute::value</methodname>
    <void/>
   </methodsynopsis>
  <para>
   Özniteliğin değerini döndürür.
  </para>
 </refsect1>
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>- Bir düğümün tüm özniteliklerini çıktılamak</title>
    <programlisting role="php">
<![CDATA[
<?php

include("example.inc");

if (!$dom = domxml_open_mem($xmlstr)) {
     echo "Belge çözümlenirken hata oluştu\n";
     exit;
}

$root = $dom->document_element();
$attrs = $root->attributes();

echo $root->node_name() . " elemanının öznitelikleri\n";
foreach ($attrs as $attribute) {
     echo ' - ' . $attribute->name . ': ' . $attribute->value . "\n";
}

?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
chapter elemanının öznitelikleri
 - language: en
]]>
    </screen>
   </example>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Özniteliğin değeri.
  </para>
 </refsect1>
 <refsect1 role="migration">
  <title>PHP 5&apos;e yükseltme</title>
  <para>
   <classname>DOMAttr</classname> nesnesinin <property>value</property>
   özelliğini kullanın.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>DomAttribute::set_value</methodname></member>
    <member><methodname>DomAttribute::name</methodname></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

